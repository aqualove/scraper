\t\tfunction genDessin(lePgm){gen_Dessin( lePgm, window );}\r\n\r\n\t\tfunction ajusteTourEtDate()\r\n\t\t{\r\n\t\t\tvar noTournee, daTournee, lMot, lLien, lHref, tStr;\r\n\t\t\tif (leProgramme == \"Q\")\r\n\t\t\t{\r\n\t\t\t\tnoTournee = Qualo; daTournee = QualoDates;\r\n\t\t\t\tlMot = QualoMot; lLien = QualoLien; lHref = QualoHref;\r\n\t\t\t}\r\n\t\t\telse\r\n\t\t\t{\r\n\t\t\t\tif (leProgramme == \"R\")\r\n\t\t\t\t{\r\n\t\t\t\t\tnoTournee = Ruisso; daTournee = RuissoDates;\r\n\t\t\t\t\tlMot = RuissoMot; lLien = RuissoLien; lHref = RuissoHref;\r\n\t\t\t\t}\r\n\t\t\t\telse\r\n\t\t\t\t{\r\n\t\t\t\t\tif (leProgramme == \"C\")\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tnoTournee = Courdo; daTournee = CourdoDates;\r\n\t\t\t\t\tlMot = CourdoMot; lLien = CourdoLien; lHref = CourdoHref;\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\tdocument.getElementById(\"plugNoTour\").innerHTML = \"Tourn&eacute;e : \" + noTournee;\r\n\t\t\tdocument.getElementById(\"plugDateTour\").innerHTML = daTournee;\r\n\t\t\tif (lMot == \"null\") lMot = \"\";\r\n\t\t\ttStr= lMot;\r\n\t\t\tif (lLien != \"null\" && lHref != \"null\")\r\n\t\t\t\ttStr += \"<A target='MOT_WIN' href=\\\"\" + lHref + \"\\\">\" + lLien + \"</A>\";\r\n\t\t\tdocument.getElementById(\"plugMotDuJour\").innerHTML = tStr;\r\n\r\n\t\t}\r\n\r\n\t\tfunction ajusteRadios(pVal)\r\n\t\t{\r\n\t\t\tvar ret, m1;\r\n\t\t\tret = true;\r\n\r\n\t\t\tif (pVal == \"Q\")\r\n\t\t\t{\r\n\t\t\t\tif (Qualo <= 0)\r\n\t\t\t\t{\r\n\t\t\t\t\tret = false;\r\n\t\t\t\t\tdocument.getElementById( \"qMenu\" ).src = menuGris[0].src;\r\n\t\t\t\t\tif (Qualo < 0)\r\n\t\t\t\t\t\tm1 = \"Il n'y a pas de programme QUALO cette ann\u00e9e\";\r\n\t\t\t\t\telse\r\n\t\t\t\t\t\tm1 = \"Il n'y a pas encore de donn\u00e9es QUALO disponibles pour cette ann\u00e9e\";\r\n\t\t\t\t}\r\n\t\t\t\telse\r\n\t\t\t\t{\r\n\t\t\t\t\tdocument.getElementById( \"qMenu\" ).src = menuNoir[0].src;\r\n\t\t\t\t\tdocument.getElementById( \"mMenu\" ).src = menuGris[3].src;\r\n\t\t\t\t\tdocument.getElementById( \"eMenu\" ).src = menuGris[4].src;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\telse\r\n\t\t\t{\r\n\t\t\t\tif (pVal == \"R\")\r\n\t\t\t\t{\r\n\t\t\t\t\tif (Ruisso <= 0)\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tret = false;\r\n\t\t\t\t\t\tdocument.getElementById( \"rMenu\" ).src = menuGris[1].src;\r\n\t\t\t\t\t\tif (Ruisso < 0)\r\n\t\t\t\t\t\t\tm1 = \"Il n'y a pas de programme RUISSO cette ann\u00e9e\";\r\n\t\t\t\t\t\telse\r\n\t\t\t\t\t\t\tm1 = \"Il n'y a pas encore de donn\u00e9es RUISSO disponibles pour cette ann\u00e9e\";\r\n\t\t\t\t\t}\r\n\t\t\t\t\telse\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tdocument.getElementById( \"rMenu\" ).src = menuNoir[1].src;\r\n\t\t\t\t\t\tdocument.getElementById( \"mMenu\" ).src = menuGris[3].src;\r\n\t\t\t\t\t\tdocument.getElementById( \"eMenu\" ).src = menuGris[4].src;\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t\telse\r\n\t\t\t\t{\r\n\t\t\t\t\tif (Courdo <= 0)\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tret = false;\r\n\t\t\t\t\t\tdocument.getElementById( \"cMenu\" ).src = menuGris[2].src;\r\n\t\t\t\t\t\tif (Courdo < 0)\r\n\t\t\t\t\t\t\tm1 = \"Il n'y a pas de programme COURDO cette ann\u00e9e\";\r\n\t\t\t\t\t\telse\r\n\t\t\t\t\t\t\tm1 = \"Il n'y a pas encore de donn\u00e9es COURDO disponibles pour cette ann\u00e9e\";\r\n\t\t\t\t\t}\r\n\t\t\t\t\telse\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tdocument.getElementById( \"cMenu\" ).src = menuNoir[2].src;\r\n\t\t\t\t\t\tdocument.getElementById( \"mMenu\" ).src = menuNoir[3].src;\r\n\t\t\t\t\t\tdocument.getElementById( \"eMenu\" ).src = menuBleu[4].src;\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\tif (ret == false) alert(m1);\r\n\t\t\treturn ret;\r\n\t\t}\r\n\r\n\t\tfunction changeProg(pVal)\r\n\t\t{\r\n\t\t\tif (pVal == \"E\" )\r\n\t\t\t{\r\n\t\t\t\tif ( leProgramme != \"C\" ) return false;\r\n\t\t\t\tif (Emis <= 0)\r\n\t\t\t\t{\r\n\t\t\t\t\tif (Courdo < 0)\r\n\t\t\t\t\t\talert(\"Il n'y a pas d'\u00e9chantillonnage des \u00e9missaires cette ann\u00e9e\");\r\n\t\t\t\t\telse\r\n\t\t\t\t\t\talert(\"Il n'y a pas encore de donn\u00e9es d'\u00e9missaires disponibles pour cette ann\u00e9e\");\r\n\t\t\t\t}\r\n\t\t\t\telse\r\n\t\t\t\t{\r\n\t\t\t\t\tafficherEmissaires();\r\n\t\t\t\t}\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t\tif (ajusteRadios(pVal) == true)\r\n\t\t\t{\r\n\t\t\t\tleProgramme = pVal;\r\n\t\t\t\tajusteTourEtDate();\r\n\t\t\t\tgenDessin(leProgramme);\r\n\t\t\t}\r\n//\t\t\tretireFiltreQualite();\r\n\t\t}\r\n\r\n/* Contr\u00f4le du menu des programmes */\r\nvar leControle;\r\nvar lImage = new Image();\r\n// Q=0;R=1;C=2;M=3;E=4\r\n\t\tfunction survolMenu( o )\r\n\t\t{\r\n\t\t\tvar p = o.id.substr( 0, 1 ).toUpperCase();\r\n\t\t\tif ( p == \"M\" ) return false;\r\n\t\t\tif ( p == leProgramme ) return false;\r\n\t\t\tif ( leProgramme != \"C\" && p == \"E\" ) return false;\r\n\t\t\tleControle = o;\r\n\t\t\tlImage.src = o.src;\r\n\t\t\tif      (p==\"Q\") o.src = menuRouge[0].src;\r\n\t\t\telse if (p==\"R\") o.src = menuRouge[1].src;\r\n\t\t\telse if (p==\"C\") o.src = menuRouge[2].src;\r\n\t\t\telse if (p==\"E\") o.src = menuRouge[4].src;\r\n\t\t\treturn false;\r\n\t\t}\r\n\r\n\t\tfunction quitteMenu( o )\r\n\t\t{\r\n\t\t\tvar p = o.id.substr( 0, 1 ).toUpperCase();\r\n\t\t\tif ( p == \"M\" ) return false;\r\n\t\t\tif ( p != leProgramme ) leControle.src = lImage.src;\r\n\t\t\treturn false;\r\n\t\t}\r\n\r\n\t\tfunction choisiMenu( o )\r\n\t\t{\r\n\t\t\tvar p = o.id.substr( 0, 1 ).toUpperCase();\r\n\t\t\tif ( p == \"M\" || p == leProgramme ) return false;\r\n\t\t\tchangeProg( p );\r\n\t\t\tif ( p == \"Q\" && Qualo > 0 )\r\n\t\t\t{\r\n\t\t\t\tdocument.getElementById( \"qMenu\" ).src = menuNoir[0].src;\r\n\t\t\t\tRuisso>0 ? document.getElementById( \"rMenu\" ).src = menuBleu[1].src :  document.getElementById( \"rMenu\" ).src = menuGris[1].src ;\r\n\t\t\t\tCourdo>0 ? document.getElementById( \"cMenu\" ).src = menuBleu[2].src :  document.getElementById( \"cMenu\" ).src = menuGris[2].src ;\r\n\t\t\t\treturn false;\r\n\t\t\t}\r\n\t\t\tif ( p == \"R\" && Ruisso > 0 )\r\n\t\t\t{\r\n\t\t\t\tQualo>0 ? document.getElementById( \"qMenu\" ).src = menuBleu[0].src :  document.getElementById( \"qMenu\" ).src = menuGris[0].src ;\r\n\t\t\t\tdocument.getElementById( \"rMenu\" ).src = menuNoir[1].src;\r\n\t\t\t\tCourdo>0 ? document.getElementById( \"cMenu\" ).src = menuBleu[2].src :  document.getElementById( \"cMenu\" ).src = menuGris[2].src ;\r\n\t\t\t\treturn false;\r\n\t\t\t}\r\n\t\t\tif ( p == \"C\" && Courdo > 0 )\r\n\t\t\t{\r\n\t\t\t\tQualo>0 ? document.getElementById( \"qMenu\" ).src = menuBleu[0].src :  document.getElementById( \"qMenu\" ).src = menuGris[0].src ;\r\n\t\t\t\tRuisso>0 ? document.getElementById( \"rMenu\" ).src = menuBleu[1].src :  document.getElementById( \"rMenu\" ).src = menuGris[1].src ;\r\n\t\t\t\tdocument.getElementById( \"cMenu\" ).src = menuNoir[2].src;\r\n\t\t\t\treturn false;\r\n\t\t\t}\r\n\t\t\treturn false;\r\n\t\t}\r\n\t\t</script>\r\n\t</head>\r\n\t<body>\r\n<!-- ATTENTION:  AVEC NETSCAPE, HEIGHT ET WIDTH NE SONT PAS MODIFIABLES, on laisse le browser s'arranger avec  -->\r\n\t\t<script type=\"text/javascript\">\r\n\t\t\tif (aspError.length)\r\n\t\t\t{\r\n\t\t\t\talert(aspError);\r\n\t\t\t}\r\n\t\t</script>\r\n\r\n\t\t<div style=\"position: absolute; left: 0px; top: 0px; visibility: visible;\">\r\n\t\t\t<img src=\"images/rsmaweb.gif\" border=\"0\" bordercolor=\"0\" width=\"720\" height=\"500\" alt=\"\" usemap=\"#FILTRES\">\r\n\t\t</div>\r\n<!-- menu:debut -->\r\n\t\t<img id=\"qMenu\" onmouseover=\"survolMenu(this);\" onmouseout=\"quitteMenu(this);\" onclick=\"choisiMenu(this);\" style=\"position:absolute;left:104px;top:46px;\" src=\"images/logoQualo_Gris.gif\"         width=\"100\" height=\"31\" border=\"0\">\r\n\t\t<img id=\"rMenu\" onmouseover=\"survolMenu(this);\" onmouseout=\"quitteMenu(this);\" onclick=\"choisiMenu(this);\" style=\"position:absolute;left:205px;top:46px;\" src=\"images/logoRuisso_Gris.gif\"        width=\"100\" height=\"31\" border=\"0\">\r\n\t\t<img id=\"cMenu\" onmouseover=\"survolMenu(this);\" onmouseout=\"quitteMenu(this);\" onclick=\"choisiMenu(this);\" style=\"position:absolute;left:306px;top:46px;\" src=\"images/logoCourdo_Gris.gif\"        width=\"100\" height=\"31\" border=\"0\">\r\n\t\t<img id=\"mMenu\" onmouseover=\"survolMenu(this);\" onmouseout=\"quitteMenu(this);\" onclick=\"choisiMenu(this);\" style=\"position:absolute;left:415px;top:47px;\" src=\"images/logoCourdoMtl_Gris.gif\"     width=\"130\" height=\"15\" border=\"0\">\r\n\t\t<img id=\"eMenu\" onmouseover=\"survolMenu(this);\" onmouseout=\"quitteMenu(this);\" onclick=\"choisiMenu(this);\" style=\"position:absolute;left:415px;top:64px;\" src=\"images/logoCourdoStation_Gris.gif\" width=\"130\" height=\"15\" border=\"0\">\r\n\t\t<script type=\"text/javascript\">\r\n\t\t\tif ( Qualo  > 0 ) document.getElementById( \"qMenu\" ).src = menuBleu[0].src;\r\n\t\t\tif ( Ruisso > 0 ) document.getElementById( \"rMenu\" ).src = menuBleu[1].src;\r\n\t\t\tif ( Courdo > 0 ) document.getElementById( \"cMenu\" ).src = menuBleu[2].src;\r\n\t\t</script>\r\n<!-- menu:fin -->\r\n\t\t<div id=\"plugMotDuJour\"  class=\"trou3\" style=\"position:absolute; left: 104px; top: 88px; width:440px;\"></div>\r\n<!-- filtres:d\u00e9but -->\r\n\t\t<map name=\"FILTRES\">\r\n\t\t\t<area shape=\"circle\" coords=\"20,222,5\"       onclick=\"appliqueFiltreQualite(0)\">\r\n\t\t\t<area shape=\"circle\" coords=\"20,242,5\"       onclick=\"appliqueFiltreQualite(1)\">\r\n\t\t\t<area shape=\"circle\" coords=\"20,262,5\"       onclick=\"appliqueFiltreQualite(2)\">\r\n\t\t\t<area shape=\"circle\" coords=\"20,281,5\"       onclick=\"appliqueFiltreQualite(3)\">\r\n\t\t\t<area shape=\"circle\" coords=\"20,302,5\"       onclick=\"appliqueFiltreQualite(4)\">\r\n\t\t\t<area shape=\"circle\" coords=\"20,320,5\"       onclick=\"appliqueFiltreQualite(5)\">\r\n\t\t\t<area shape=\"rect\"   coords=\"10,177,139,331\" onclick=\"retireFiltreQualite()\">\r\n\t\t\t<area shape=\"rect\"   coords=\"43,281,99,292\"  onclick=\"changeProg('E');\">\r\n\t\t\t<area shape=\"rect\"   coords=\"2,455,100,474\"  onclick=\"afficherMiseEnGarde()\">\r\n\t\t\t<area shape=\"rect\"   coords=\"2,475,100,494\"  onclick=\"afficherModeDemploi()\">\r\n\t\t</map>\r\n<!-- filtres:fin --> \r\n\t\t<div id=\"plugNoTour\" class=\"trou\" style=\"left: 12px; top: 135px;\"></div>\r\n\t\t<div id=\"plugDateTour\" class=\"trou\" style=\"left: 12px; top: 151px;\"></div>\r\n<!-- minifiche:d\u00e9but -->\r\n\t\t<!--  pr\u00e9sentation des donn\u00e9es des stations -->\r\n\t\t<div id=\"nomstation\" align=center class=\"trou1\" style=\"position: absolute; left: 151px; top: 143px; width: 80px; height: 13px;\">STATION</div>\r\n\t\t<div id=\"descL1\"     class=\"trou2\"               style=\"position: absolute; left: 240px; top: 137px; width: 165px; height: 13px;\"></div>\r\n\t\t<div id=\"descL2\"     class=\"trou2\"               style=\"position: absolute; left: 240px; top: 151px; width: 165px; height: 13px;\"></div>\r\n\t\t<div id=\"dateprelevement\"   class=\"fiche\" style=\"position: absolute; left: 187px; top: 174px;\"></div>\r\n\t\t<div id=\"moyennecoliformes\" class=\"fiche\" style=\"position: absolute; left: 242px; top: 200px;\"></div>\r\n\t\t<div id=\"temperature\"       class=\"fiche\" style=\"position: absolute; left: 257px; top: 226px;\"></div>\r\n\t\t<div id=\"meteo\" style=\"position: absolute; left: 320px; top: 201px; visibility: hidden\">\r\n\t\t\t<img id=\"imgmeteo\" src=\"images/meteoTS.gif\" width=\"83\" height=\"45\" border=\"0\">\r\n\t\t</div>\r\n\t\t<img id=\"boitecliquer\" src=\"images/boitecliquer.gif\" width=\"260\" height=\"18\" border=\"0\" style=\"position: absolute; left: 149px; top: 248px; visibility: hidden\">\r\n<!-- minifiche:fin -->\r\n\t\t<div id=\"Dessin\" name=\"Dessin\">&nbsp;</div>\r\n\t\t<script type=\"text/javascript\">\r\n\t\t\tif (leProgramme.length)\r\n\t\t\t{\r\n\t\t\t\tajusteTourEtDate();\r\n\t\t\t\tajusteRadios(leProgramme);\r\n\t\t\t\tgenDessin(leProgramme);\r\n//\t\t\t\tretireFiltreQualite();\r\n\t\t\t}\r\n\t\t</script>\r\n\t</body>\r\n</html>\r\n", "encoding": 